msgid "'x' must be a numeric vector"
msgstr "'x' doit être un vecteur numérique"

msgid "'y' must be a numeric vector"
msgstr "'y' doit être un vecteur numérique"

msgid "'x' and 'y' must have same length"
msgstr "'x' et 'y' doivent avoir la même longueur"

msgid "'model' must be 1, 2, -2, 3 or -3"
msgstr "'model' doit être 1, 2, -2, 3 ou -3"

msgid "'var_known' must be TRUE, FALSE, 1 or 0"
msgstr "'var_known' doit être TRUE, FALSE, 1 ou 0"

msgid "'weights' must be a numeric vector or matrix"
msgstr "'weights' doit être un vecteur ou matrice numérique"

msgid "matrix 'weights' must be  n x n  where  n = length(x)"
msgstr ""
"le matrice 'weights' doit être de dimension  n par n,  où  n = length(x)"

msgid "vector 'weights' must have same length as 'x'"
msgstr "le vecteur 'weights' doit avoir la même longueur que 'x'"

