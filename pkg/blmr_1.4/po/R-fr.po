msgid "'x' must be a numeric vector"
msgstr "'x' doit être un vecteur numérique"

msgid "'y' must be a numeric vector"
msgstr "'y' doit être un vecteur numérique"

msgid "'x' and 'y' must have same length"
msgstr "'x' et 'y' doivent avoir la même longueur"

msgid "'S' must be a numeric vector or matrix"
msgstr "'S' doit être un vecteur ou matrice numérique"

msgid "'model' must be 1, 2 or -2"
msgstr "'model' doit être 1, 2 ou -2"

msgid "'var_known' must be TRUE, FALSE, 1 or 0"
msgstr "'var_known' doit être TRUE, FALSE, 1 ou 0"

msgid "matrix 'S' must be n x n where n=length(x)"
msgstr "le matrice 'S' doit être de dimension  n par n,  où  n = length(x)"

msgid "vector 'S' must have same length as 'x'"
msgstr "le vecteur 'S' doit avoir la même longueur que 'x'"

